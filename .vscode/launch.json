  "version": "0.2.0",
  "configurations": [
    {
      "name": "configure-accounts",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "env": {
        "VAULT_ADDR": "https://vault.example.com",
        "VAULT_NAMESPACE": "namespace",
        "VAULT_UTIL_ACCOUNTS": "{\"aws\":[\"chef-cd\"],\"github\":[\"chef\"]}"
      },
      "envFile": "${workspaceFolder}/.env",
      "program": "${workspaceFolder}",
      "args": ["configure-accounts"]
    },
    {
      "name": "fetch-secret-env",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "env": {
        "VAULT_ADDR": "https://vault.example.com",
        "VAULT_NAMESPACE": "namespace",
        "VAULT_UTIL_SECRETS": "{\"TEST\":{\"path\":\"secret/hello\",\"field\":\"foo\"}}"
      },
      "envFile": "${workspaceFolder}/.env",
      "program": "${workspaceFolder}",
      "args": ["fetch-secret-env"]
    },
    {
      "name": "print-git-credentials",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "env": {
        "VAULT_ADDR": "https://vault.example.com",
        "VAULT_NAMESPACE": "namespace",
      },
      "envFile": "${workspaceFolder}/.env",
      "program": "${workspaceFolder}",
      "args": ["print-git-credentials", "--app", "chef"]
    }
  ]
}